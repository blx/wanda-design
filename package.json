{
  "name": "@wonderflow/design",
  "description": "The Wonderflow Design System website.",
  "version": "0.1.0",
  "private": true,
  "author": "Wonderflow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/design.git"
  },
  "keywords": [
    "design-system",
    "wonderflow",
    "website"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/design/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/design#readme",
  "scripts": {
    "start": "next dev",
    "build": "next build && next export -o dist",
    "format": "prettier --check \"**/*.{css,html,js,json,md,yml}\"",
    "format:write": "prettier --write \"**/*.{css,html,js,json,md,yml}\"",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint \"src/**/*.js\"",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:commits": "commitlint --edit",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "postcss": "^8.3.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.3.2",
    "prettier-config-standard": "^4.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0"
  }
}
