{
  "name": "@wonderflow/design",
  "description": "The Wonderflow Design System website.",
  "version": "0.0.0",
  "private": true,
  "author": "Wonderflow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/design.git"
  },
  "keywords": [
    "design-system",
    "wonderflow",
    "website"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/design/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/design#readme",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 1 Safari versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "start": "next dev",
    "build": "next build && next export -o dist",
    "format": "prettier --check \"**/*.{json,md,yml}\"",
    "format:write": "prettier --loglevel silent --write \"**/*.{json,md,yml}\"",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint src/**/*.{jsx,js} pages/**/*.jsx",
    "lint:styles": "stylelint src/**/*.css pages/**/*.css",
    "lint:commits": "commitlint --edit",
    "prepare": "husky install",
    "release": "semantic-release",
    "update:cssdb": "npx browserslist@latest --update-db"
  },
  "dependencies": {
    "@next/bundle-analyzer": "11.0.1",
    "@wonderflow/tokens": "1.1.0",
    "clsx": "1.1.1",
    "cssnano": "5.0.6",
    "cssnano-preset-default": "5.1.3",
    "flat": "5.0.2",
    "next": "11.0.1",
    "next-compose-plugins": "2.2.1",
    "postcss": "8.3.5",
    "postcss-easy-import": "3.0.0",
    "postcss-inset": "1.0.0",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "7.0.0",
    "prettier": "2.3.2",
    "prettier-config-standard": "4.0.0",
    "semantic-release": "17.4.4",
    "stylelint": "13.13.1",
    "stylelint-config-equinusocio": "1.0.5",
    "stylelint-design-tokens-plugin": "0.0.12"
  }
}
